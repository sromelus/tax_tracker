      <a href="/">
        <%= image_tag'https://s3.amazonaws.com/tax-tracker-development/Logo.png', class:"img"  %>
      </a>
      <ul>
        <% if @user %>
          <li>
              <%= @user.first_name %>
              <%= @user.last_name %>
          </li>
          <li>
            <a>
              <%= link_to 'Sign Out', destroy_user_session_path, method: :delete, class:"button-home-page" %>
