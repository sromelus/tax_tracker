
<div class="container-signin-signup">
  <div class="container-inside">
    <nav>
      <a href="/">
        <%= image_tag'https://s3.amazonaws.com/tax-tracker-development/Logo.png', class:"img"  %>
      </a>
    </nav>


    <div class="main-signin-signup">
      <section>
         <h2>Forgot your password?</h2>
         <div class="form">
           <%= form_for(resource, as: resource_name, url: password_path(resource_name), html: { method: :post }) do |f| %>
             <%= render "devise/shared/error_messages", resource: resource %>
             <%= f.label "email:", :class => 'label' %><br>
             <%= f.email_field :email, autofocus: true, autocomplete: "email", :class => 'input' %><br>
             <%= f.submit "Send me reset password instructions", :class => 'button' %>
           <% end %>
           <p>
             <%= render "devise/shared/links" %>
           </p>
         </div>
      </section>
    </div>
  </div>
</div>
<footer class="footer">
  <ul>
    <li><a><%= link_to 'About Us', aboutUs_path %></a></li>
    <li><a><%= link_to 'Contact Us', contact_path %></a></li>
  </ul>
</footer>
